{
  "name": "@dnd/backend",
  "version": "0.0.1",
  "description": "Game server for dnd",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "biome format src/ --write",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "biome lint src/",
    "lint:fix": "biome lint src/ --apply",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "typeorm": "ts-node --project ./tsconfig.json ./../../node_modules/typeorm/cli.js",
    "migration:run": "npm run typeorm -- -d ./src/config/typeorm.config.ts migration:run",
    "migration:revert": "npm run typeorm -- -d ./src/config/typeorm.config.ts migration:revert",
    "migration:generate": "npm run typeorm -- -d ./src/config/typeorm.config.ts migration:generate ./src/database/migrations/$npm_config_name"
  },
  "dependencies": {
    "@dnd/shared": "file:../shared",
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.8",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/platform-socket.io": "^10.3.8",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.3.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "nestjs-zod": "^3.0.0",
    "pg": "^8.11.5",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "zod": "^3.23.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.2",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.8",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.2",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.8",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^1.5.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "unplugin-swc": "^1.4.5",
    "vitest": "^1.5.3"
  }
}
