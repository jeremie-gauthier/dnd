{
  "name": "@dnd/backend",
  "version": "0.0.1",
  "description": "Game server for dnd",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "typeorm": "ts-node --project ./tsconfig.json ./../../node_modules/typeorm/cli.js",
    "migration:run": "npm run typeorm -- -d ./src/config/typeorm.ts migration:run",
    "migration:generate": "npm run typeorm -- -d ./src/config/typeorm.ts migration:generate ./src/database/migrations/$npm_config_name"
  },
  "dependencies": {
    "@dnd/shared": "file:../shared",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/platform-socket.io": "^10.3.0",
    "@nestjs/typeorm": "^10.0.1",
    "@nestjs/websockets": "^10.3.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "nestjs-zod": "^3.0.0",
    "pg": "^8.11.3",
    "redis": "^4.6.12",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@dnd/prettier-config": "^0.0.0",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.3.0",
    "@swc/core": "^1.3.101",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/supertest": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitest/coverage-v8": "^1.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.1",
    "prettier": "^3.1.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "unplugin-swc": "^1.4.4",
    "vitest": "^1.1.0"
  }
}
