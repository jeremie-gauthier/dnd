{
  "name": "@dnd/frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start:dev": "concurrently \"npm run start:css\" \"tsr watch\" \"VITE_ENV=localhost vite --force\"",
    "start:css": "tailwindcss -i ./src/main.css -o ./src/generated.css --watch",
    "start:dev:network": "concurrently \"tsr watch\" \"VITE_ENV=local_network vite --force --host\"",
    "build": "tsc && npm run build:css && vite build --debug",
    "build:css": "NODE_ENV=production tailwindcss -o src/tailwind.css -m",
    "check": "biome check src/",
    "check:fix": "biome check src/ --write",
    "preview": "vite preview",
    "format": "biome format src/ --write"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@dnd-kit/core": "^6.1.0",
    "@dnd/shared": "file:../shared",
    "@headlessui/react": "^2.1.8",
    "@heroicons/react": "^2.1.5",
    "@radix-ui/react-tooltip": "^1.1.3",
    "@tanstack/react-form": "^0.33.0",
    "@tanstack/react-query": "^5.56.2",
    "@tanstack/react-router": "^1.58.3",
    "@tanstack/router-cli": "^1.58.1",
    "@tanstack/zod-form-adapter": "^0.33.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "i18next": "^23.15.1",
    "i18next-http-backend": "^2.6.1",
    "lucide-react": "^0.447.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.2",
    "socket.io-client": "^4.8.0",
    "tailwind-merge": "^2.5.3",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.2",
    "@tanstack/react-query-devtools": "^5.56.2",
    "@tanstack/router-devtools": "^1.58.3",
    "@tanstack/router-plugin": "^1.58.4",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "concurrently": "^9.0.1",
    "tailwindcss": "^3.4.12",
    "typescript": "^5.6.2",
    "vite": "^5.4.7",
    "vite-tsconfig-paths": "^5.0.1"
  }
}
