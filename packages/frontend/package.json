{
  "name": "@dnd/frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start:dev": "concurrently \"tsr watch\" \"VITE_ENV=localhost vite --force\"",
    "start:dev:network": "concurrently \"tsr watch\" \"VITE_ENV=local_network vite --force --host\"",
    "build": "tsc && vite build --debug",
    "lint": "biome lint src/",
    "lint:fix": "biome lint src/ --apply",
    "preview": "vite preview",
    "format": "biome format src/ --write"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@dnd/shared": "file:../shared",
    "@tanstack/react-form": "^0.14.0",
    "@tanstack/react-query": "^5.28.4",
    "@tanstack/react-router": "^1.20.1",
    "@tanstack/router-cli": "^1.20.0",
    "@tanstack/zod-form-adapter": "^0.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io-client": "^4.7.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.1",
    "@tanstack/react-query-devtools": "^5.28.4",
    "@tanstack/router-devtools": "^1.20.1",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "concurrently": "^8.2.2",
    "typescript": "^5.4.2",
    "vite": "^5.1.6"
  }
}
