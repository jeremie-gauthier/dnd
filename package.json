{
  "name": "dnd",
  "private": true,
  "version": "1.0.0",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "start:dev:db": "docker-compose -f ./packages/backend/docker-compose.yml up -d --build",
    "start:dev:server": "npm run start:dev --workspace=@dnd/backend",
    "start:dev": "npm run start:dev:db && npm run start:dev:server"
  }
}
